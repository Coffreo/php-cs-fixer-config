{
    "name": "coffreo-toolbox/php-cs-fixer-config",
    "description": "Provides a configuration factory and multiple Coffreo rule sets for friendsofphp/php-cs-fixer.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Andreas Möller",
            "email": "am@localheinz.com"
        },
        {
            "name": "Cyril Méry",
            "email": "cmery@coffreo.com"
        }
    ],
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "require": {
        "php": "^5.4 || ^5.6 || ^7.0",
        "friendsofphp/php-cs-fixer": "^2.2.4 || ^2.3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.5"
    },
    "autoload": {
        "psr-4": {
            "Coffreo\\PhpCsFixer\\Config\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Coffreo\\PhpCsFixer\\Config\\Test\\": "test"
        }
    }
}
